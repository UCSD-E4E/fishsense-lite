    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "fishsense_lite",
            "justMyCode": false,
            "args": [
                "process",
                "~/data/**/*.ORF",
                "--lens-calibration",
                "~/data/fsl-01d-lens-raw.pkg",
                "--laser-calibration",
                "~/data/laser-calibration.pkg",
                "--output",
                "./output/results.db",
                // "--overwrite"
            ]
        }
    ]
}