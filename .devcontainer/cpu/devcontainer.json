{
	"image": "ghcr.io/ucsd-e4e/fishsense:cpu",
	"postCreateCommand": "poetry config cache-dir ${containerWorkspaceFolder}/poetry_cache && poetry config virtualenvs.create false && poetry install --with dev",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.vscode-pylance",
				"ms-python.python",
				"ms-toolsai.jupyter",
				"njpwerner.autodocstring",
				"ms-python.isort",
				"eamodio.gitlens",
				"VisualStudioExptTeam.vscodeintellicode",
				"github.vscode-github-actions",
				"ms-python.black-formatter",
				"ms-azuretools.vscode-docker"
			]
		}
	}
}